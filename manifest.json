{
  "manifest_version": 3,
  "name": "获取网页词频",
  "version": "1.0",
  "description": "本项目基于chrome-extensions-samples-main修改。本项目使用MIT许可。本项目是一个Chrome扩展，用来分析网站词频",
"background": {
    "service_worker": "js/background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
 "content_scripts": [
   {
     "matches": ["<all_urls>"],
     "js": ["js/content.js"]
   }
 ],
"chrome_url_overrides": {
  "newtab": "welcome.html"
},
"icons": {"48": "icon48.png",
          "128": "icon192.png" },
  "permissions": [
      "tabs",
"activeTab"
  ]
}
